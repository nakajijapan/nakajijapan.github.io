		dropBox.addEventListener(&#34;dragover&#34;, dandd.dragOver, false);
		dropBox.addEventListener(&#34;drop&#34;, dandd.drop, true);
}



// ãƒ­ãƒ¼ãƒ‰æ™‚ã«å‡¦ç†ã‚’è¿½åŠ 
